<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_at_Start_of_Program__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
NOT(ISBLANK(Birthdate)) &amp;&amp;
NOT(ISBLANK($Setup.Launchpad__Participant_Community_Setup__c.Program_Start_Date__c)),
FLOOR(
($Setup.Launchpad__Participant_Community_Setup__c.Program_Start_Date__c -
Birthdate) / 365.25
),
null
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Age at Start of Program</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
