<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Applicant_s_Age_at_Start_of_Program__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
    NOT(ISBLANK(Applicant_s_Date_of_Birth_MM_DD_YYYY__c)) &amp;&amp;
    NOT(ISBLANK(Program__r.Launchpad__Program_Year_Start__c)),
    FLOOR(
        (Program__r.Launchpad__Program_Year_Start__c - 
         Applicant_s_Date_of_Birth_MM_DD_YYYY__c) / 365.25
    ),
    null
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Applicant&apos;s Age at Start of Program</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
